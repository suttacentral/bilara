{
  "name": "bilara",
  "version": "0.9.0",
  "contributors": [
    "Blake Walsh"
  ],
  "license": "BSD-3-Clause",
  "repository": "suttacentral/bilara",
  "scripts": {
    "dev": "npm run start | npm run watch",
    "start": "npx es-dev-server",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "watch": "browser-sync start --proxy localhost:8081 --port 3003 --files \"**/*.js\"",
    "test:regenerate_screenshots": "mocha test/integration/screenshots-baseline/regenerate.js --timeout=15000",
    "start:build": "npm run build && es-dev-server --root-dir build --app-index index.html --open --compatibility none"
  },
  "dependencies": {
    "@lion/dialog": "^0.16.1",
    "@webcomponents/webcomponentsjs": "^2.7.0",
    "commander": "^9.4.1",
    "express-http-proxy": "^1.6.3",
    "fix": "^0.0.6",
    "lit-element": "^3.2.2",
    "pwa-helpers": "^0.9.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "reselect": "^4.1.7"
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^2.2.1",
    "axe-core": "^4.5.1",
    "browser-sync": "^2.27.10",
    "chai": "^4.3.7",
    "del": "^7.0.0",
    "es-dev-server": "^2.1.0",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "koa-proxies": "^0.12.2",
    "mocha": "^10.1.0",
    "pixelmatch": "^5.3.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-copy": "^2.0.0",
    "wct-mocha": "^1.1.0"
  }
}
